    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^14.1.0",
    "concurrently": "^3.3.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
