{
  "name": "jsgames2",
  "version": "1.0.0",
  "description": "A more object oriented iteration of JSGames",
  "main": "entry.js",
  "scripts": {
    "server": "concurrently --kill-others \"gulp watch\" \"node entry.js\" \"node-sass --output-style compressed -w app/sass/main.scss dist/main.css\"",
    "bundle-js": "gulp bundle",
    "bundle-css": "node-sass --output-style compact app/sass/main.scss dist/main.css",
    "start": "node entry.js",
    "watch": "gulp watch",
    "postinstall": "gulp bundle && node-sass --output-style compact app/sass/main.scss dist/main.css",
    "lint": "gulp lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rigaudon/jsgames2.git"
  },
  "author": "Michael Wu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rigaudon/jsgames2/issues"
  },
  "homepage": "https://github.com/Rigaudon/jsgames2#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "backbone": "^1.3.3",
    "backbone.marionette": "^3.1.0",
    "bluebird": "^3.5.1",
    "bootstrap-toggle": "^2.2.2",
    "bufferutil": "^2.0.1",
    "dateformat": "^2.0.0",
    "dotenv": "^4.0.0",
    "engine.io": "^2.0.2",
    "express": "^4.14.1",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "node-emoji": "^1.5.1",
    "progressbar.js": "^1.0.1",
    "socket.io": "^1.7.3",
